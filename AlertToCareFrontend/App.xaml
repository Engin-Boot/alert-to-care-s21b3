<Application x:Class="AlertToCareFrontend.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/styles/MainWindowStyles.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Resources/styles/ConfigurationPageStyles.xaml"></ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Resources/styles/BedStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
